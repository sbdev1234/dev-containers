# http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 80
trim_trailing_whitespace = true

[*.ts]
max_line_length = 120
single_quote = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
